{
	"name": "sveltekit-starter",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:db:start": "docker run -d --name sveltekit-starter-pg -e POSTGRES_PASSWORD=postgres -p 5432:5432 postgres:16.2",
		"dev:db:reset": "docker rm -f sveltekit-starter-pg && pnpm dev:db:start",
		"dev:db:stop": "docker stop sveltekit-starter-pg",
		"build": "npx @inlang/paraglide-js@latest  compile --project ./project.inlang --outdir ./src/lib/paraglide && vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"studio": "drizzle-kit studio",
		"generate": "drizzle-kit generate",
		"migrate": "drizzle-kit push",
		"postinstall": "npx @inlang/paraglide-js@latest compile --project ./project.inlang --outdir ./src/lib/paraglide",
		"seed": "npx tsx ./src/lib/server/database/seed.ts",
		"translate": "npx @inlang/cli machine translate --project ./project.inlang"
	},
	"devDependencies": {
		"@faker-js/faker": "^9.3.0",
		"@inlang/paraglide-js": "1.11.3",
		"@inlang/paraglide-sveltekit": "^0.14.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-node": "^5.2.9",
		"@sveltejs/kit": "^2.10.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.1",
		"@tailwindcss/typography": "^0.5.15",
		"@types/eslint": "^9.6.1",
		"@types/nodemailer": "^6.4.17",
		"@types/pg": "^8.11.10",
		"@typescript-eslint/eslint-plugin": "^8.18.0",
		"@typescript-eslint/parser": "^8.18.0",
		"autoprefixer": "^10.4.20",
		"bits-ui": "1.0.0-next.71",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.7",
		"drizzle-kit": "^0.30.0",
		"eslint": "^9.16.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"formsnap": "2.0.0-next.1",
		"lucide-svelte": "^0.468.0",
		"mode-watcher": "^0.5.0",
		"pg": "^8.13.1",
		"postcss": "^8.4.49",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"svelte": "5.11.2",
		"svelte-check": "^4.1.1",
		"svelte-copy": "^2.0.0",
		"svelte-headless-table": "^0.18.3",
		"svelte-sonner": "^0.3.28",
		"sveltekit-flash-message": "^2.4.4",
		"sveltekit-search-params": "^3.0.0",
		"sveltekit-superforms": "^2.21.1",
		"tailwind-merge": "^2.5.5",
		"tailwind-variants": "^0.3.0",
		"tailwindcss": "^3.4.16",
		"tailwindcss-animate": "^1.0.7",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2",
		"vite": "^6.0.3",
		"vitest": "^2.1.8",
		"zod": "^3.24.1"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-ses": "^3.709.0",
		"@node-rs/argon2": "^1.1.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"drizzle-orm": "^0.38.1",
		"nanoid": "^5.0.9",
		"nodemailer": "^6.9.16",
		"oslo": "^1.2.1"
	}
}
