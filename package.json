{
	"name": "sveltekit-starter",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:db:start": "docker run -d --name sveltekit-starter-pg -e POSTGRES_PASSWORD=postgres -p 5432:5432 postgres:16.2",
		"dev:db:reset": "docker rm -f sveltekit-starter-pg && pnpm dev:db:start",
		"dev:db:stop": "docker stop sveltekit-starter-pg",
		"build": "npx @inlang/paraglide-js@latest  compile --project ./project.inlang --outdir ./src/lib/paraglide && vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"studio": "drizzle-kit studio",
		"generate": "drizzle-kit generate",
		"migrate": "drizzle-kit push",
		"postinstall": "npx @inlang/paraglide-js@latest compile --project ./project.inlang --outdir ./src/lib/paraglide",
		"seed": "npx tsx ./src/lib/server/database/seed.ts",
		"translate": "npx @inlang/cli machine translate --project ./project.inlang"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@inlang/paraglide-js": "1.11.2",
		"@inlang/paraglide-sveltekit": "^0.11.0",
		"@playwright/test": "^1.47.1",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/kit": "^2.5.20",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tailwindcss/typography": "^0.5.13",
		"@types/eslint": "^9.6.0",
		"@types/nodemailer": "^6.4.15",
		"@types/pg": "^8.11.6",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"autoprefixer": "^10.4.20",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.23.1",
		"eslint": "^9.8.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"lucia": "^3.2.0",
		"pg": "^8.12.0",
		"postcss": "^8.4.40",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "5.0.0-next.208",
		"svelte-check": "^3.8.5",
		"svelte-copy": "^1.4.2",
		"svelte-headless-table": "^0.18.2",
		"sveltekit-flash-message": "^2.4.4",
		"sveltekit-search-params": "^3.0.0",
		"tailwindcss": "^3.4.7",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"vite": "^5.3.5",
		"vitest": "^2.0.5"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-ses": "^3.623.0",
		"@lucia-auth/adapter-drizzle": "^1.0.7",
		"bits-ui": "^0.21.13",
		"clsx": "^2.1.1",
		"cmdk-sv": "^0.0.18",
		"drizzle-orm": "^0.32.1",
		"formsnap": "^1.0.1",
		"lucide-svelte": "^0.424.0",
		"mode-watcher": "^0.4.1",
		"nanoid": "^5.0.7",
		"nodemailer": "^6.9.14",
		"oslo": "^1.2.1",
		"svelte-sonner": "^0.3.27",
		"sveltekit-superforms": "^2.16.1",
		"tailwind-merge": "^2.4.0",
		"tailwind-variants": "^0.2.1",
		"zod": "^3.23.8"
	}
}
