{
	"name": "sveltekit-starter",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "docker start sveltekit-starter-pg && vite dev",
		"dev:db:start": "docker run -d --name sveltekit-starter-pg -e POSTGRES_PASSWORD=postgres -p 5432:5432 postgres:16.2",
		"dev:db:reset": "docker rm -f sveltekit-starter-pg && pnpm dev:db:start",
		"dev:db:stop": "docker stop sveltekit-starter-pg",
		"build": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"studio": "drizzle-kit studio",
		"generate": "drizzle-kit generate",
		"migrate": "drizzle-kit push",
		"postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
		"seed": "npx tsx ./src/lib/server/database/seed.ts",
		"translate": "npx @inlang/cli machine translate --project ./project.inlang"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@inlang/paraglide-js": "1.9.0",
		"@inlang/paraglide-sveltekit": "^0.7.0",
		"@playwright/test": "^1.44.0",
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/kit": "^2.5.9",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@tailwindcss/typography": "^0.5.13",
		"@types/eslint": "^8.56.10",
		"@types/nodemailer": "^6.4.15",
		"@typescript-eslint/eslint-plugin": "^7.10.0",
		"@typescript-eslint/parser": "^7.9.0",
		"autoprefixer": "^10.4.19",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.21.2",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.39.0",
		"lucia": "^3.2.0",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"svelte": "5.0.0-next.136",
		"svelte-check": "^3.7.1",
		"svelte-copy": "^1.4.2",
		"svelte-headless-table": "^0.18.2",
		"sveltekit-flash-message": "^2.4.4",
		"sveltekit-search-params": "^2.1.2",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.11",
		"vitest": "^1.6.0"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-ses": "^3.577.0",
		"@lucia-auth/adapter-drizzle": "^1.0.7",
		"bits-ui": "^0.21.8",
		"clsx": "^2.1.1",
		"cmdk-sv": "^0.0.17",
		"drizzle-orm": "^0.30.10",
		"formsnap": "^1.0.0",
		"lucide-svelte": "^0.378.0",
		"mode-watcher": "^0.3.0",
		"nanoid": "^5.0.7",
		"nodemailer": "^6.9.13",
		"oslo": "^1.2.0",
		"postgres": "^3.4.4",
		"svelte-sonner": "^0.3.24",
		"sveltekit-superforms": "^2.13.1",
		"tailwind-merge": "^2.3.0",
		"tailwind-variants": "^0.2.1",
		"zod": "^3.23.8"
	}
}
